{
    "name": "vongola12324/laravel-color-hash",
    "description": "PHP Port of ColorHash Javascript Library for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords":[
	"laravel", "color", "hash"
    ],
    "homepage": "https://github.com/vongola12324/laravel-color-hash",
    "authors": [
	{
	    "name": "Vongola",
	    "email": "vongola12324@coder.tw"
	}
    ],
    "require": {
	"shahonseven/php-color-hash": "dev-master#82e555cb75558704d1537a9dc4ea3d2d898a84e5",
	"illuminate/support":"^5.0"
    },
    "require-dev": {
	"phpunit/phpunit": "^7.0"
    },
     "scripts": {
	"test": "phpunit"
     },
    "autoload": {
	"psr-4": {
	    "Vongola\\ColorHash\\": "src/"
	}
    },
    "extra": {
	"component": "package",
	"laravel": {
	    "providers": [
		"Vongola\\ColorHash\\ColorHashServiceProvider"
	    ],
	    "aliases": {
		"ColorHash": "Vongola\\ColorHash\\Facades\\ColorHashFacade"
	    }
	}
    }
}
